 [Tests]
  design = 'Problem/index.md'
  parallel_scheduling = True

  [steady_no_converge]
    type = 'RunApp'
    input = 'steady_no_converge.i'
    expect_out = 'Aborting as solve did not converge'

    requirement = 'The system shall report an error when the system solver is unable to find a converged solution.'
    design = 'Executioner/index.md'
    issues = '#5144 #5153'
    collections = 'FAILURE_ANALYSIS'
  []

