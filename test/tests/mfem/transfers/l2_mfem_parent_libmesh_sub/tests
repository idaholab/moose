[Tests]
  design = 'MultiAppMFEMlibMeshGeneralFieldTransfer.md'
  issues = '#31982'
  requirement = 'The system shall have the ability to transfer libMesh solutions to MFEM for '
  [MFEMTransferFromlibMeshL2Quad]
    type = CSVDiff
    input = mfem_parent_scalar.i
    csvdiff = 'mfem_parent_libmesh_sub_scalar_quads.csv'
    detail = ' scalar MONOMIAL variables defined on quads, '
    abs_zero = 1e-8
    capabilities = 'mfem'
    recover = false
    compute_devices = 'cpu cuda'
  []
  [MFEMTransferFromlibMeshL2Tri]
    type = CSVDiff
    input = mfem_parent_scalar.i
    csvdiff = 'mfem_parent_libmesh_sub_scalar_tris.csv'
    requirement = 'constant MONOMIAL variables defined on tris, '
    abs_zero = 1e-8
    capabilities = 'mfem'
    recover = false
    compute_devices = 'cpu cuda'
    cli_args = 'Mesh/file=../../mesh/square_tri.e '
               'Outputs/file_base=mfem_parent_libmesh_sub_scalar_tris '
               'libmesh_app0:Mesh/file=../../mesh/square_tri.e '
  []
  [MFEMTransferFromlibMeshL2TriToQuad]
    type = CSVDiff
    input = mfem_parent_scalar.i
    csvdiff = 'mfem_parent_libmesh_sub_scalar_tri_to_quad.csv'
    requirement = 'constant MONOMIAL variables from tris to quads, '
    abs_zero = 1e-8
    capabilities = 'mfem'
    recover = false
    compute_devices = 'cpu cuda'
    cli_args = 'Mesh/file=../../mesh/square.e '
               'Outputs/file_base=mfem_parent_libmesh_sub_scalar_tri_to_quad '
               'libmesh_app0:Mesh/file=../../mesh/square_tri.e '
  []
  [MFEMTransferFromlibMeshL2Hex]
    type = CSVDiff
    input = mfem_parent_scalar.i
    csvdiff = 'mfem_parent_libmesh_sub_scalar_hexes.csv'
    requirement = 'constant MONOMIAL variables defined on hexes, '
    abs_zero = 1e-8
    capabilities = 'mfem'
    recover = false
    compute_devices = 'cpu cuda'
    cli_args = 'Mesh/file=../../mesh/cube.e '
               'Outputs/file_base=mfem_parent_libmesh_sub_scalar_hexes '
               'libmesh_app0::Mesh/file=../../mesh/cube.e '
  []  
  [MFEMTransferFromlibMeshL2Tet]
    type = CSVDiff
    input = mfem_parent_scalar.i
    csvdiff = 'mfem_parent_libmesh_sub_scalar_tets.csv'
    requirement = 'constant MONOMIAL variables defined on tetrahedra, '
    abs_zero = 1e-8
    capabilities = 'mfem'
    recover = false
    compute_devices = 'cpu cuda'
    cli_args = 'Mesh/file=../../mesh/cube_tetmesh.e '
               'Outputs/file_base=mfem_parent_libmesh_sub_scalar_tets '
               'libmesh_app0::Mesh/file=../../mesh/cube_tetmesh.e '
  []
  [MFEMTransferFromlibMeshL2TetToHex]
    type = CSVDiff
    input = mfem_parent_scalar.i
    csvdiff = 'mfem_parent_libmesh_sub_scalar_tet_to_hex.csv'
    requirement = 'and constant MONOMIAL variables from tets to hexes.'
    abs_zero = 1e-8
    capabilities = 'mfem'
    recover = false
    compute_devices = 'cpu cuda'
    cli_args = 'Mesh/file=../../mesh/cube.e '
               'Outputs/file_base=mfem_parent_libmesh_sub_scalar_tet_to_hex '
               'libmesh_app0::Mesh/file=../../mesh/cube_tetmesh.e '
  []      
