    issues = "#7885"
    design = "InterfaceKernels/index.md"
    requirement = "The InterfaceKernel system shall operate with coupled variables in 2D. "
                  "This uses a penalty implementation that is optimally convergent."
  [../]

  [./test_dot]
    type = 'Exodiff'
