  [./second_lagrange_err]
    type = 'RunException'
    group = 'contact_lagrange'
    input = 'error-input.i'
    cli_args = 'Variables/disp_x/order=SECOND Outputs/exodus=false Mesh/second_order=true Contact/leftright/lm=lm'
    expect_err = "The current lagrange multiplier implementation currently relies on first-order "
              "Lagrange shape functions for the displacements in order to generate the best "
              "preconditioner."
  [../]
  [./not_coulomb_err]
    type = 'RunException'
    group = 'contact_lagrange'
    input = 'frictional-two-body-slip.i'
    cli_args = 'Contact/leftright/model=frictionless Outputs/exodus=false'
    expect_err = "The current tangential lagrange multiplier implementation only supports the "
                   "Coulomb frictional model."
  [../]
  [./not_frictionless_err]
    type = 'RunException'
    group = 'contact_lagrange'
    input = 'two-body-dirichlet.i'
    cli_args = 'Contact/leftright/model=coulomb Outputs/exodus=false'
    expect_err = "The model options for lagrange multiplier options are frictionless or Coulomb "
                 "with coupled tangential lagrange multipliers."
  [../]
  [./lm_not_coupled_err]
    type = 'RunException'
    group = 'contact_lagrange'
    input = 'error-input.i'
    cli_args = 'Outputs/exodus=false'
    expect_err = "If using the Lagrange formulation, you must couple in a lagrange multiplier variable"
  [../]
  [./wrong_dim_err]
    type = 'RunException'
    group = 'contact_lagrange'
    input = 'error-input.i'
    cli_args = "Contact/leftright/lm=lm Mesh/dim=1 Outputs/exodus=false GlobalParams/displacements='disp_x'"
    expect_err = "The current lagrange multiplier implementation only works in two dimensions."
