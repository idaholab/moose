    input = 'MatGradSquareCoupled.i'
    exodiff = 'MatGradSquareCoupled_out.e'
  [../]

  [./analyzejacobian_AllenCahn]
    type = 'AnalyzeJacobian'
    input = 'AllenCahn.i'
    prereq = 'CoupledAllenCahn'
    expect_out = '\nNo errors detected. :-\)\n'
    resize_mesh = true
    recover = false
