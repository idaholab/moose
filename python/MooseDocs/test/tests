[Tests]
  issues = '#6699'
  design = 'MooseDocs/index.md'
  [materialize]
    requirement = "MooseDocs shall be able to translate markdown syntax using to HTML with Materialize support:"
    [barrier]
        type = RunCommand
        command = 'python3 moosedocs.py verify --form materialize --executioner MooseDocs.base.ParallelBarrier'
        detail = "using a barrier-based parallel scheme;"
    []
    [serial]
        type = RunCommand
        command = 'python3 moosedocs.py verify --form materialize --executioner MooseDocs.base.Serial'
        detail = "using a serial scheme;"
        prereq = 'materialize/barrier'
    []
    [pipe]
        type = RunCommand
        command = 'python3 moosedocs.py verify --form materialize --executioner MooseDocs.base.ParallelPipe'
        #prereq = serial
        skip = 'WIP'
        detail = "using a pipe-based parallel scheme; and"
    []
    [demand]
        type = RunCommand
        command = 'python3 moosedocs.py verify --form materialize --executioner MooseDocs.base.ParallelDemand'
        #prereq = pipe
        skip = 'WIP'
        detail = "using a on-demand parser parallel scheme."
    []
  []

